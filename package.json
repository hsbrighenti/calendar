{
  "name": "calendar",
  "version": "0.0.1",
  "description": "Simple Calendar Web App made with React and Typescript as the Jobsity Recruitment Proccess chalenge",
  "main": "index.js",
  "scripts": {
    "start": "parcel index.html --port 8080 --cache-dir dist/cache --open",
    "build": "parcel build index.html",
    "lint": "npm run compile && npm run eslint",
    "eslint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "compile": "tsc",
