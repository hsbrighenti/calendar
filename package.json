    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hsbrighenti/calendar.git"
  },
  "keywords": [
    "React",
    "TypeScript",
    "Calendar",
    "Jobsity"
  ],
  "author": "Henrique S. Brighenti",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hsbrighenti/calendar/issues"
  },
  "homepage": "https://github.com/hsbrighenti/calendar#readme",
  "devDependencies": {
    "@testing-library/react": "^11.0.2",
    "@types/jest": "^26.0.13",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "eslint": "^7.8.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-testing-library": "^3.8.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.2",
    "parcel-bundler": "^1.12.4",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "src": "^1.1.2"
